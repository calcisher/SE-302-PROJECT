<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClassAssignmentSystem.StudentScheduleController">
    <TableView fx:id="scheduleTable" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn text="timeColumn" fx:id="timeColumn" />
            <TableColumn text="mondayColumn" fx:id="mondayColumn" />
            <TableColumn text="tuesdayColumn" fx:id="tuesdayColumn" />
            <TableColumn text="wednesdayColumn" fx:id="wednesdayColumn" />
            <TableColumn text="thursdayColumn" fx:id="thursdayColumn" />
            <TableColumn text="fridayColumn" fx:id="fridayColumn" />
        </columns>
    </TableView>
</VBox>
